{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket"
        ],
       "Resource": [
          "arn:aws:s3:::dev.ckk.cloud"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
          "s3:PutObject",
          "s3:GetObject",
          "s3:DeleteObject",
          "s3:PutObjectAcl"
        ],
        "Resource": [
           "arn:aws:s3:::dev.ckk.cloud/*"
        ]
      }
    ]
  }